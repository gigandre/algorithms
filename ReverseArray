import java.util.Arrays;

public class ReverseArray {

	public static void main(String[] args) {
		int[] arr = {1, 2, 3, 4, 5, 1, 1, 2, 4, 5, 5};

		int i = 0;
		int j = arr.length - 1;

		while(i < j) {
			int t = arr[i];
			arr[i] = arr[j];
			arr[j] = t;
			i++;
			j--;
		}

		System.out.println(Arrays.toString(arr));

	}

}
